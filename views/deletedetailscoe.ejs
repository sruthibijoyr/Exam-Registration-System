<%- include('partials/header') %>

<%- include('partials/navbarcoe') %>

<div class="cover-box">
  <div class="animated bounceInDown">
    <div class="container-delete">
      <span class="animated tada"></span>
          <form name="form3" class="box" action="/confirmdelete" method="POST">
            <h4>Search<span>Result</span></h4>
            <br>
            <% if(ecode == "0"){%>
              <h5>No matching results.</h5>
            <%}else{%>
              <table class="exam-details">
                <tr>
                  <th>Exam Code</th>
                  <td><%= ecode %></td>
                </tr>
                <tr>
                  <th>Exam Department</th>
                  <td><%= edept %></td>
                </tr>
                <tr>
                  <th>Exam Name</th>
                  <td><%= ename %></td>
                </tr>
                <tr>
                  <th>Exam Date</th>
                  <td><%= edate %></td>
                </tr>
                <tr>
                  <th>Exam Session</th>
                  <td><%= esess %></td>
                </tr>
                <tr>
                  <th>Number of registrations</th>
                  <td><%= num %></td>
                </tr>
                <input type="hidden" name="code" value="<%= ecode %>">
              </table>
              <input type="submit" class="btn1" name="btn-1" value="Confirm Delete">
            <%}%>

          </form>
          <a href="/viewdelete" class="search-go-back">Go Back</a>
    </div>
  </div>
</div>

<%- include('partials/footer') %>
